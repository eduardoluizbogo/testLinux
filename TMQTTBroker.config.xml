<TMQTTBrokerConfiguration>
	<GlobalSettings>
		<Backlog>100</Backlog>
		<DisableRetainedFile>false</DisableRetainedFile>
		<MaxPendingMessagesPerClientout>1000</MaxPendingMessagesPerClientout>
		<UserCredentials>
			<User Name="" Password=""/>
			<User Name="" Password=""/>
		</UserCredentials>
		<CLientIDs>
			<CliendID ClientName="" Topic="" CanSubscribe="" CanPublish=""/>
			<CliendID ClientName="" Topic="" CanSubscribe="" CanPublish=""/>
		</CLientIDs>
	</GlobalSettings>
	
	<LocalSettings>
		<ListeningPort>1883</ListeningPort>
		<CertificateFile></CertificateFile>
		<CertificatePassword></CertificatePassword>
		<SSLProtocols>None</SSLProtocols>
	</LocalSettings>
		
	<RemoteSettings>
		<RemoteBrokerURL></RemoteBrokerURL>
		<RemotePort>1883</RemotePort>
		<RemoteCertificateFile></RemoteCertificateFile>
		<RemoteCertificatePasword></RemoteCertificatePasword>
		<RemoteSSLProtocols>None</RemoteSSLProtocols>
		<RemoteWebSocket>false</RemoteWebSocket>
		<RemoteKeepAlive>100</RemoteKeepAlive>
		<RemoteAllowUntrustedCertificates>true</RemoteAllowUntrustedCertificates>
	</RemoteSettings>

	<LinkedSolutions>	
		<GroupId></GroupId>
		
		<TServersInfo>
			<TServerInfo SolutionName="" IpAddress="" Port=""/>
			<TServerInfo SolutionName="" IpAddress="" Port=""/>
		</TServersInfo>
			
		<TagProperties>
			<Property></Property>
		</TagProperties>
	
	</LinkedSolutions>
	
</TMQTTBrokerConfiguration>